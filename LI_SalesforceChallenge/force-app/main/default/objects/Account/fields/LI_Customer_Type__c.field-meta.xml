<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LI_Customer_Type__c</fullName>
    <description>Returns the customer type based on Annual revenue spends</description>
    <externalId>false</externalId>
    <formula>IF(AnnualRevenue  &gt;= 50000,&apos;GOLD&apos;, IF(AnnualRevenue  &gt;= 25000,&apos;SILVER&apos;, IF(AnnualRevenue  &gt;= 10000,&apos;BRONZE&apos;, &apos;-&apos;) ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Customer Type</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
